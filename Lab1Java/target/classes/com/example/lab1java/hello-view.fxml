<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.canvas.Canvas?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.lab1java.HelloController">
    <Canvas fx:id="canvas" width="700" height="700"/>
    <Button text="Select Paint" fx:id="selectPaintButton" onAction="#handleFileSelection"/>
    <Button text="Select Crossword" fx:id="selectCrosswordButton" onAction="#handleCrosswordSelection"/>
</VBox>
